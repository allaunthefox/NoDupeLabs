{
  "subagents": {
    "researcher": {
      "role": "Research Agent",
      "description": "Specialized in codebase analysis, file exploration, and information gathering",
      "capabilities": [
        "read_file",
        "search_files",
        "list_files",
        "list_code_definition_names"
      ],
      "restrictions": [
        "No file modifications",
        "No command execution",
        "Read-only access"
      ]
    },
    "developer": {
      "role": "Development Agent",
      "description": "Handles code implementation, file creation, and modifications",
      "capabilities": [
        "write_to_file",
        "replace_in_file",
        "execute_command"
      ],
      "restrictions": [
        "No authentication operations",
        "Project-scoped operations only"
      ]
    },
    "tester": {
      "role": "Testing Agent",
      "description": "Focuses on test execution, validation, and quality assurance",
      "capabilities": [
        "execute_command",
        "read_file"
      ],
      "restrictions": [
        "Test-related operations only",
        "No production modifications"
      ]
    },
    "analyst": {
      "role": "Analysis Agent",
      "description": "Performs code analysis, pattern detection, and architectural review",
      "capabilities": [
        "search_files",
        "list_code_definition_names",
        "read_file"
      ],
      "restrictions": [
        "Read-only operations",
        "No modifications"
      ]
    }
  },
  "default_subagent": "researcher",
  "project_specific": {
    "NoDupeLabs": {
      "context_window_limit": 150000,
      "preferred_subagents": ["researcher", "developer"],
      "specialized_rules": {
        "context_management": "Use /compact when approaching context limits",
        "testing_framework": "pytest",
        "development_style": "Direct and technical communication"
      }
    }
  }
}
